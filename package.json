{
	"name": "svelte-jsreport",
	"version": "0.0.4",
	"main": "electron.cjs",
	"author": "Jayson Gwapo",
	"description": "Hello World!",
	"repository": {
		"type": "git",
		"url": "https://github.com/jayson-corotan/elctron-svelte-update.git"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"electron-dev": "concurrently --kill-others \"vite dev\" \"electron electron.cjs\"",
		"electron": "electron electron.cjs",
		"start": "electron-forge start",
		"package": "electron-forge package",
		"make": "electron-forge make",
		"publish": "electron-forge publish"
	},
	"devDependencies": {
		"@electron-forge/cli": "^6.2.1",
		"@electron-forge/maker-deb": "^6.2.1",
		"@electron-forge/maker-rpm": "^6.2.1",
		"@electron-forge/maker-squirrel": "^6.2.1",
		"@electron-forge/maker-zip": "^6.2.1",
		"@electron-forge/plugin-auto-unpack-natives": "^6.2.1",
		"@electron-forge/publisher-github": "^6.2.1",
		"@fontsource/fira-mono": "^4.5.10",
		"@neoconfetti/svelte": "^1.0.0",
		"@ptkdev/sveltekit-electron-adapter": "^0.3.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.20.4",
		"@types/cookie": "^0.5.1",
		"electron": "^25.2.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.0.0",
		"svelte-check": "^3.4.3",
		"typescript": "^5.0.0",
		"vite": "^4.3.6"
	},
	"type": "module",
	"dependencies": {
		"concurrently": "^8.2.0",
		"electron-serve": "^1.1.0",
		"electron-squirrel-startup": "^1.0.0",
		"electron-window-state": "^5.0.3",
		"jsreport": "^3.13.0",
		"update-electron-app": "^2.0.1"
	}
}
